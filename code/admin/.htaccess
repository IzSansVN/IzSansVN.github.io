#File bảo vệ htaccess
<Files .htaccess>
Order Allow,Deny
Deny from all
</Files>

RewriteEngine on
# Xem và chỉnh sửa liên kết nếu muốn
RewriteRule ^duyet-the.html duyet-the.php
RewriteRule ^index.html index.php

# php -- BEGIN cPanel-generated handler, do not edit
